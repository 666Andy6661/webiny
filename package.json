{
	"name": "webiny",
	"version": "0.1.0",
	"private": true,
	"packageManager": "yarn@3.3.1",
	"dependencies": {
		"@webiny/aws-layers": "5.34.0",
		"@webiny/cli": "5.34.0",
		"@webiny/cli-plugin-deploy-pulumi": "5.34.0",
		"@webiny/cli-plugin-scaffold": "5.34.0",
